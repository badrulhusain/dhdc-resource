{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist/spa",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "cleanUrls": true
}